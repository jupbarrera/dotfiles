#Some ls aliases
  alias ll='ls -alF'
  alias la='ls -A'
  alias l='ls -CF'

  alias clr='clear'
#open vim instead of vi
  alias vi='vim'
 
#Somo git aliases

alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '
alias gk='gitk --all&'
alias gx='gitx --all'

